# Bluesky Authentication Credentials
# Copy this file to .env and fill in your actual credentials
# Get your app password from: https://bsky.app/settings/app-passwords

BLUESKY_USERNAME=your-handle.bsky.social
BLUESKY_PASSWORD=your-app-password-here

# Target account to scrape
TARGET_ACCOUNT=reformexposed.bsky.social

# Optional: Number of posts to fetch (set to "ALL" or leave blank for all posts)
POST_LIMIT=ALL
